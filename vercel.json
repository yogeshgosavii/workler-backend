{
	"version": 2,
	"public": true,
	"builds": [
		{
			"src": "./index.js",
			"use": "@vercel/node"
		}
	],
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "./index.js"
		}
	],
	"headers": [
		{
			"source": "/api/profile/education/",  
			"headers": [
			{ "key": "Access-Control-Allow-Origin", "value": "https://workler-frontend.vercel.app" },  
			{ "key": "Access-Control-Allow-Methods", "value": "GET" },
			{ "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }, 
			{ "key": "Access-Control-Allow-Credentials", "value": "true" } 
			]
		}
]
}